// Punctuation
equals      = "=";
rbracket    = "]";
lbracket    = "[";
colon       = ":";
dot         = ".";
comma       = ",";
rparen      = ")";
lparent     = "(";
exclamation = "!";
percent     = "%";
hyphen      = "-";
comment     = "//";

